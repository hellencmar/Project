<Window x:Class="View.ListarAnotacaoCompleta"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:View"
        mc:Ignorable="d"
        Title="ListarAnotacaoCompleta" Height="450" Width="800">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="51,49,0,0" TextWrapping="Wrap" Text="Título" VerticalAlignment="Top" FontSize="20"/>
        <TextBox x:Name="txtTitulo" HorizontalAlignment="Left" Height="23" Margin="51,81,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="240"/>
        <TextBlock HorizontalAlignment="Left" Margin="52,130,0,0" TextWrapping="Wrap" Text="Descrição" VerticalAlignment="Top" FontSize="20"/>
        <TextBox x:Name="txtDescricao" HorizontalAlignment="Left" Height="192" Margin="52,162,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="479"/>

    </Grid>
</Window>
